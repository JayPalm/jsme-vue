{
  "name": "jsme-vue",
  "main": "./dist/jsme-vue.umd.js",
  "module": "/dist/jsme-vue.es.js",
  "exports": {
    ".": {
      "import":"./dist/jsme-vue.es.js",
      "require":"./dist/jsme-vue.umd.js"
    }
  },
  "style": "/dist/jsme-vue.css",
  "files": [
    "dist/*",
    "README.md",
    "LICENSE"
  ],
  "private": false,
  "version": "0.0.9",
  "description": "Vue wrapper for JSME-Editor",
  "keywords": [
    "chemistry",
    "molecule-structure",
    "molecule",
    "smiles"
  ],
  "license": "3-clause BSD",
  "homepage": "https://github.com/JayPalm/jsme-vue",
  "repository": {
    "type": "git",
    "url": "https://github.com/JayPalm/jsme-vue"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "jsme-editor": "^2022.2.26",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vue-tsc": "^0.39.5"
  }
}
